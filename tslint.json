{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": ["node_modules/**/*.ts", "src/registerServiceWorker.js"]
  },
  "defaultSeverity": "warning",
  "rules": {
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "jsx-no-lambda": false,
    "no-console": true,
    "no-debugger": true,
    "array-type": false,
    "import-name": false
  },
  "jsRules": {
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "no-console": true,
    "variable-name": false
  }
}
