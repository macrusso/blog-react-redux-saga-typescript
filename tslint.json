{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "jsx-no-lambda": false,
    "no-console": false,
    "array-type": false
  },
  "jsRules": {
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "no-console": false,
    "variable-name": false
  }
}